{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "aideo",
  "version": "0.1.0",
  "identifier": "com.eugene.aideo",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:5317",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "aideo",
        "width": 480,
        "height": 640,
        "minWidth": 360,
        "minHeight": 480,
        "maxWidth": 600,
        "maxHeight": 900,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
